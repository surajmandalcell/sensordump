{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "preview:apk": {
      "android": {
        "buildType": "apk",
         "autoIncrement": true
      }
    },
    "preview:gradle": {
      "android": {
        "gradleCommand": ":app:assembleRelease",
         "autoIncrement": true
      }
    },
    "preview:dev": {
      "developmentClient": true,
       "autoIncrement": true
    },
    "preview:distribute": {
      "distribution": "internal",
       "autoIncrement": true
    },
    "production-apk": {
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      }
    },
    "production-aab": {
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      }
    }
  }
}
